{
	"name": "@bookworm/eventcatalog",
	"version": "1.0.0",
	"author": "Nhan Nguyen <nguyenxuannhan407@gmail.com> (https://github.com/foxminchan)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/foxminchan/BookWorm.git",
		"directory": "docs/eventcatalog"
	},
	"dependencies": {
		"@eventcatalog/core": "latest",
		"@eventcatalog/generator-asyncapi": "latest",
		"@eventcatalog/generator-openapi": "latest"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 3 chrome version",
			"last 3 firefox version",
			"last 5 safari version"
		]
	},
	"bugs": {
		"url": "https://github.com/foxminchan/BookWorm/issues"
	},
	"description": "Living event-driven architecture catalog with service contracts and documentation",
	"engines": {
		"node": ">=25.0.0"
	},
	"homepage": "https://bookwormdev.netlify.app/",
	"licenses": [
		{
			"type": "MIT",
			"url": "https://opensource.org/licenses/MIT"
		}
	],
	"private": true,
	"scripts": {
		"prepare": "allow-scripts",
		"dev": "eventcatalog dev",
		"build": "eventcatalog build",
		"start": "eventcatalog start",
		"preview": "eventcatalog preview",
		"generate": "eventcatalog generate",
		"lint": "eventcatalog-linter",
		"format": "prettier --write .",
		"check": "eventcatalog-linter && prettier --check ."
	},
	"lavamoat": {
		"allowScripts": {
			"@eventcatalog/core>@asyncapi/react-component>@asyncapi/protobuf-schema-parser>protobufjs": false,
			"@eventcatalog/core>@parcel/watcher": false,
			"@eventcatalog/core>@scalar/api-reference-react>@scalar/api-reference>@vueuse/core>vue-demi": false,
			"@eventcatalog/core>astro-compress>astro>esbuild": false,
			"@eventcatalog/core>astro-compress>sharp": false,
			"@eventcatalog/core>astro>esbuild": false,
			"@eventcatalog/core>astro>sharp": false,
			"@eventcatalog/core>astro>vite>esbuild": false
		}
	},
	"devDependencies": {
		"@eventcatalog/linter": "^1.0.6",
		"@lavamoat/allow-scripts": "^3.4.3",
		"prettier": "^3.8.1"
	}
}
