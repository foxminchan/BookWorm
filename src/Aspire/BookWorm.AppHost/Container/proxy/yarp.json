{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "catalogApi": {
        "ClusterId": "catalogApi",
        "Match": {
          "Path": "/catalog/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/catalog"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      },
      "chatApi": {
        "ClusterId": "chatApi",
        "Match": {
          "Path": "/chatting/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/chatting"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      },
      "basketApi": {
        "ClusterId": "basketApi",
        "Match": {
          "Path": "/basket/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/basket"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      },
      "orderingApi": {
        "ClusterId": "orderingsApi",
        "Match": {
          "Path": "/ordering/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/ordering"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      },
      "ratingApi": {
        "ClusterId": "ratingApi",
        "Match": {
          "Path": "/rating/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/rating"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      },
      "financeApi": {
        "ClusterId": "financeApi",
        "Match": {
          "Path": "/finance/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/finance"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      },
      "identityApi": {
        "ClusterId": "identityApi",
        "Match": {
          "Path": "/identity/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/identity"
          },
          {
            "PathPrefix": "/"
          },
          {
            "RequestHeaderOriginalHost": "true"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "trace-id"
          },
          {
            "X-Forwarded": "Append",
            "HeaderPrefix": "TraceId"
          }
        ]
      }
    },
    "Clusters": {
      "catalogApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://catalog",
            "Health": "http+https://catalog/health"
          }
        }
      },
      "chatApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://chatting",
            "Health": "http+https://chatting/health"
          }
        }
      },
      "basketApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://basket",
            "Health": "http+https://basket/health"
          }
        }
      },
      "orderingApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://ordering",
            "Health": "http+https://ordering/health"
          }
        }
      },
      "ratingApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://rating",
            "Health": "http+https://rating/health"
          }
        }
      },
      "financeApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://finance",
            "Health": "http+https://finance/health"
          }
        }
      },
      "identityApi": {
        "Destinations": {
          "base_destination": {
            "Address": "http+https://keycloak"
          }
        }
      }
    }
  }
}
