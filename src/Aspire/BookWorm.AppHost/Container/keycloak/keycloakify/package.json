{
  "name": "bookworm-keycloakify",
  "version": "1.0.0",
  "description": "A Keycloak theme for BookWorm",
  "author": "Nhan Nguyen",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "postinstall": "keycloakify sync-extensions",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-keycloak-theme": "bun run build && keycloakify build",
    "storybook": "storybook dev -p 6006",
    "format": "biome format --write",
    "check": "biome check",
    "lint": "biome lint --write",
    "ci": "biome ci"
  },
  "dependencies": {
    "keycloakify": "^11.11.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^3.4.18",
    "@keycloakify/email-native": "~260007.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@storybook/react-vite": "^10.0.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "globals": "^16.5.0",
    "storybook": "^10.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.1"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  }
}
