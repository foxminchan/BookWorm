{
  "name": "@bookworm/clients",
  "version": "1.0.0",
  "description": "Frontend clients for BookWorm application",
  "author": "Nhan Nguyen <nguyenxuannhan407@gmail.com> (https://github.com/foxminchan)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxminchan/BookWorm.git",
    "directory": "src/Clients"
  },
  "bugs": {
    "url": "https://github.com/foxminchan/BookWorm/issues"
  },
  "homepage": "https://foxminchan.github.io/BookWorm/",
  "scripts": {
    "prepare": "allow-scripts",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "e2e": "turbo run test:e2e",
    "boundaries": "turbo boundaries",
    "format": "prettier --write \"**/*.{ts,tsx,md,feature}\""
  },
  "lavamoat": {
    "allowScripts": {}
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^3.4.3",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.8.1",
    "prettier-plugin-gherkin": "^3.1.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.8.11",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.30.3",
  "engines": {
    "node": ">=25.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@scarf/scarf",
      "core-js-pure",
      "esbuild",
      "msw",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "semver": ">=7.5.2",
      "prismjs": ">=1.30.0",
      "js-yaml": ">=4.1.1",
      "lodash-es": ">=4.17.23",
      "tmp": ">=0.2.4",
      "minimatch": ">=10.2.1",
      "langsmith": ">=0.4.6"
    }
  }
}
