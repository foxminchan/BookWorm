{
  "name": "@bookworm/backoffice",
  "version": "1.0.0",
  "type": "module",
  "description": "Administration backoffice for BookWorm online bookstore",
  "author": "Nhan Nguyen <nguyenxuannhan407@gmail.com> (https://github.com/foxminchan)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxminchan/BookWorm.git",
    "directory": "src/Clients/apps/backoffice"
  },
  "bugs": {
    "url": "https://github.com/foxminchan/BookWorm/issues"
  },
  "homepage": "https://foxminchan.github.io/BookWorm/",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "bddgen": "npx bddgen",
    "bddgen:watch": "npx bddgen --watch",
    "bddgen:export": "npx bddgen export",
    "test:e2e": "npx bddgen && npx playwright test",
    "test:e2e:headed": "npx bddgen && npx playwright test --headed",
    "test:e2e:ui": "npx bddgen && npx playwright test --ui",
    "test:e2e:ci": "npx bddgen && npx playwright test --workers=4",
    "playwright:install": "playwright install --with-deps chromium",
    "playwright:install:all": "playwright install --with-deps",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.212.0",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@opentelemetry/sdk-trace-node": "^2.5.1",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-table": "^8.21.3",
    "@workspace/api-client": "workspace:*",
    "@workspace/api-hooks": "workspace:*",
    "@workspace/mocks": "workspace:*",
    "@workspace/types": "workspace:*",
    "@workspace/ui": "workspace:*",
    "@workspace/utils": "workspace:*",
    "@workspace/validations": "workspace:*",
    "better-auth": "^1.4.18",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.1",
    "ts-pattern": "^5.9.0",
    "usehooks-ts": "3.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@faker-js/faker": "^10.3.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "allure-playwright": "^3.4.5",
    "eslint": "^10.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "happy-dom": "^20.6.1",
    "playwright-bdd": "^8.4.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
