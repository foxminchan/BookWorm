  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
  <Property Name="sonar.host.url">https://sonarcloud.io</Property>
  <Property Name="sonar.exclusions">
    eventcatalog/**,src/Services/**/**/Infrastructure/Migrations/**,src/Services/Ordering/BookWorm.Ordering/Internal/Generated/**
  </Property>
  <Property Name="sonar.coverage.exclusions">
    src/Services/**/**/Infrastructure/**,src/Services/**/Program.cs,src/BuildingBlocks/**,src/Integrations/**,src/BookWorm.ServiceDefaults/**,src/BookWorm.AppHost/**,src/BookWorm.Gateway/**
  </Property>
  <Property Name="sonar.cs.vstest.reportsPaths">src/Services/**/**/test-results/*.trx</Property>
  <Property Name="sonar.cs.vscoveragexml.reportsPaths">src/Services/**/**/test-results/*.xml</Property>
</SonarQubeAnalysisProperties>
